    </record>

    <!-- Things RAS User Group -->
    <record id="things_ras_group_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id"
        ref="module_things_ras_category"/>
        <!-- <field name="implied_ids"
        eval="[(4, ref('base.group_user'))]"/> -->
