    <!-- Things RAS Manager Group -->
    <record id="things_ras_group_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id"
        ref="module_things_ras_category"/>
        <!-- <field name="implied_ids"
        eval="[(4, ref('things_ras_group_user'))]"/>
        <field name="users"
        eval="[(4, ref('base.user_root')),
        (4, ref('base.user_admin'))]"/> -->
    </record>

    <!-- <record id="group_things_ras" model="res.groups">
